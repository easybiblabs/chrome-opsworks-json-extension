{
  "manifest_version": 2,
  "description": "Nicer editor for opsworks stack settings",
  "name": "Opsworks JSON editor",
  "content_scripts": [
    {
      "matches": [
        "https://console.aws.amazon.com/opsworks/*"
      ],
      "js": [
        "jquery.min.js",
        "editor.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://console.aws.amazon.com/*"
  ],
  "version": "0.1",
  "web_accessible_resources": [
    "jsoneditor.min.css",
    "editor-iframe.html",
    "img/*.png",
    "*.js",
    "jsoneditor.map"
  ]
}
